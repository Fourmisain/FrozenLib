{
  "schemaVersion": 1,
  "id": "frozenblocklib",
  "version": "${version}",
  "name": "FrozenBlock's Library",
  "description": "It will grow over time.",
  "authors": [
	"FrozenBlock"
  ],
  "contact": {
	"homepage": "https://modrinth.com/mod/frozenlib",
	"sources": "https://github.com/FrozenBlock/FrozenLib",
	"issues": "https://github.com/FrozenBlock/FrozenLib/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/frozenblocklib/icon.png",
  "environment": "*",
  "entrypoints": {
	"main": [
	  "net.frozenblock.lib.fabric.FrozenMainFabric"
	],
	"client": [
	  "net.frozenblock.lib.fabric.FrozenClientFabric"
	],
	"frozenlib:events": [
	  "net.frozenblock.lib.fabric.FrozenMainFabric"
	],
	"preLaunch": [
	  "net.frozenblock.lib.fabric.FrozenPreLaunchFabric"
	]
  },
  "mixins": [
	"mixin/frozenblocklib-common.mixins.json",
	"mixin/frozenblocklib-fabric.mixins.json",
	"mixin/quiltmc_datafixerupper_frozenblock-common.mixins.json",
	"mixin/quiltmc_resource_loader_frozenblock-common.mixins.json",
	"mixin/quiltmc_surface_rule_frozenblock-common.mixins.json"
  ],
  "depends": {
	"fabricloader": ">=0.14.8",
	"fabric": ">=0.58.0",
	"minecraft": ">=1.19",
	"java": ">=17"
  },
  "breaks": {
    "quilted_fabric_api": "4.0.0-beta.8"
  },
  "custom": {
	"modmenu": {
	  "links": {
		"modmenu.discord": "https://discord.gg/EpkBWm844s"
	  },
	  "badges": [
		"library"
	  ]
	}
  }
}
