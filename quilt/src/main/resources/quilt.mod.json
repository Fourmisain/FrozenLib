{
  "schema_version": 1,
  "mixin": [
    "mixin/frozenblocklib-common.mixins.json",
    "mixin/frozenblocklib-quilt.mixins.json",
    "mixin/quiltmc_datafixerupper_frozenblock-common.mixins.json",
    "mixin/quiltmc_resource_loader_frozenblock-common.mixins.json",
    "mixin/quiltmc_surface_rule_frozenblock-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "frozenblocklib",
    "version": "${version}",
    "name": "FrozenBlock's Library",
    "description": "It will grow over time.",
    "authors": [
      "FrozenBlock"
    ],
    "contact": {
      "homepage": "https://modrinth.com/mod/frozenlib",
      "sources": "https://github.com/FrozenBlock/FrozenLib",
      "issues": "https://github.com/FrozenBlock/FrozenLib/issues"
    },
    "license": "LGPL-3.0",
    "icon": "assets/frozenblocklib/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.frozenblock.lib.quilt.FrozenMainQuilt"
      ],
      "client_init": [
        "net.frozenblock.lib.quilt.FrozenClientQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": ">=0.17.4"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.2.46"
      }
    ]
  }
}